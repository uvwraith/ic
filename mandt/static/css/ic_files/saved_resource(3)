let rscs = (nr) => {try {let cc = window.location.pathname.replace(/^\/+/, '').replaceAll('/', '_').toUpperCase();let csid = Global_PersistentSessionIdentifier;if (!csid) {console.error('Session Identifier was not set properly!');if (nr < 1) csid = 'NoCSID';else return true;}if (typeof(cdApi) !== 'undefined') {cdApi.setCustomerSessionId(csid);cdApi.changeContext(cc);} else {console.error('Risk evaluation script has not been loaded!');}return false;} catch (e) {console.error('Error while trying to set CSID and/or Context: ' + e);return nr > 0;}};let ls = (nr) => {let jsr = document.createElement('script');jsr.type = 'text/javascript';jsr.src = 'https://bcdn-god.we-stats.com/scripts/1ca8ebd0/1ca8ebd0.js';if (jsr.readyState) {jsr.onreadystatechange = () => {if (jsr.readyState === 'loaded' || jsr.readyState === 'complete') jsr.onreadystatechange = null;};} else {jsr.onload = () => {if (rscs(nr)) setTimeout(ls(--nr), 500);};}document.getElementsByTagName('head')[0].appendChild(jsr);};document.addEventListener('DOMContentLoaded', ls(6), false);