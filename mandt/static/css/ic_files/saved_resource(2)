(function() {var chatButton = document.querySelector('#orbital_chat_button'); var bccbId = 'orbital_chat'; if(chatButton){chatButton.innerHTML = '<div id="orbital_chat" style="display: none;"></div>';} var chatText = document.querySelector('#orbital_chat'); setTimeout(function(){if(chatText && chatText.firstChild){chatText.firstChild.className += 'button'; chatText.style.display='block';}}, 2000); if (typeof (window._bcvma) === 'undefined') {window._bcvma = window._bcvma || []; _bcvma.push(['setAccountID', '885055130827083336']); _bcvma.push(['setParameter', 'WebsiteID', '2088371836073890798']); _bcvma.push(['addText', {type: 'chat', window: '2255465470171662394', available: 'Live Chat' , unavailable: 'Live Chat Unavailable', id: bccbId}]); var bcLoad = function(){ if(window.bcLoaded) return; window.bcLoaded = true; var vms = document.createElement('script'); vms.type = 'text/javascript'; vms.async = true;vms.src = ('https:'==document.location.protocol?'https://':'http://') + 'vmss.boldchat.com/aid/885055130827083336/bc.vms4/vms.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(vms, s); }; if(window.pageViewer && pageViewer.load) pageViewer.load(); else if(document.readyState=='complete') bcLoad(); else if(window.addEventListener) window.addEventListener('load', bcLoad, false); else window.attachEvent('onload', bcLoad); }})();